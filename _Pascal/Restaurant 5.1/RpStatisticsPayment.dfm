  object qryStatisticsPayment: TADOQuery
    ConnectionString = 
      'Provider=SQLNCLI.1;Password=1234567890;Persist Security Info=Tru' +
      'e;User ID=sa;Initial Catalog=RestMirdamad2;Data Source=.'
    CursorType = ctStatic
    CommandTimeout = 600
    Parameters = <>
    SQL.Strings = (
      
        'select SUM(ff_price) FF_Price,SUM(ff_no) FF_Count,FoodMaxPrice,F' +
        'ood_Name, Building_Name from'
      
        '(SELECT     fishfood.FF_Date, fishfood.FF_Type, fishfood.FF_Fish' +
        'No, '
      #9#9'   fishfood.FF_Food, fishfood.FF_Price, fishfood.FF_Pay, '
      
        '           fishfood.FF_No, foodlist.Food_Type, foodlist.Food_Nam' +
        'e,'
      '           fish.Fish_PCode, fish.Fish_RdrCode, readers.R_Name, '
      '           readers.R_Station, building.Building_Name, '
      '           dbo.FoodMaxPrice(FF_Date, FF_Food, 0) as FoodMaxPrice'
      'FROM       building INNER JOIN'
      
        '           readers ON building.Building_Code = readers.R_Station' +
        ' RIGHT OUTER JOIN'
      '           fishfood INNER JOIN'
      
        '           foodlist ON fishfood.FF_Food = foodlist.Food_Code INN' +
        'ER JOIN'
      '           fish ON fishfood.FF_Date = fish.Fish_Date AND '
      '           fishfood.FF_Type = fish.Fish_Type AND '
      '           fishfood.FF_FishNo = fish.Fish_No '
      '           ON readers.R_Code = fish.Fish_RdrCode'
      'WHERE '
      
        'Fish_PCode+FF_Date+CONVERT(varchar(3),FF_FishNo)+CONVERT(varchar' +
        '(3),Food_Type) not in (select fish_pcode+ff_date+convert(varchar' +
        '(3),ff_fishno)+'#39'2'#39' from dbo.Mihman(Fish_PCode,FF_Date))'
      'and'
      ' FF_Date = '#39'1390/04/08'#39'  --and Fish_PCode='#39'00000014'#39' '
      ' '
      ''
      ''
      'union'
      ''
      ''
      ''
      
        'SELECT     fishfood.FF_Date, fishfood.FF_Type, fishfood.FF_FishN' +
        'o, '
      #9#9'    FF_Food, fishfood.FF_Price, fishfood.FF_Pay, '
      
        '           fishfood.FF_No, foodlist.Food_Type,'#39#1594#1584#1575#1740' '#1575#1590#1575#1601#1740#39' Food_' +
        'Name,'
      '           fish.Fish_PCode, fish.Fish_RdrCode, readers.R_Name, '
      '           readers.R_Station, building.Building_Name, '
      '           dbo.FoodMaxPrice(FF_Date, FF_Food, 1) as FoodMaxPrice'
      'FROM       building INNER JOIN'
      
        '           readers ON building.Building_Code = readers.R_Station' +
        ' RIGHT OUTER JOIN'
      '           fishfood INNER JOIN'
      
        '           foodlist ON fishfood.FF_Food = foodlist.Food_Code INN' +
        'ER JOIN'
      '           fish ON fishfood.FF_Date = fish.Fish_Date AND '
      '           fishfood.FF_Type = fish.Fish_Type AND '
      '           fishfood.FF_FishNo = fish.Fish_No '
      '           ON readers.R_Code = fish.Fish_RdrCode'
      'WHERE '
      
        'Fish_PCode+FF_Date+CONVERT(varchar(3),FF_FishNo)+CONVERT(varchar' +
        '(3),Food_Type) in (select fish_pcode+ff_date+convert(varchar(3),' +
        'ff_fishno)+'#39'2'#39' from dbo.Mihman(Fish_PCode,FF_Date))'
      'and'
      ' FF_Date = '#39'1390/04/08'#39' ) l10'
      ' '
      'Group by Building_Name,Food_Name,FoodMaxPrice ')
    Left = 24
    Top = 8
