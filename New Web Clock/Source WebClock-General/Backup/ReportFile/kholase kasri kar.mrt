declare  @query varchar(max)
set @query=''
declare @nameTable nvarchar(10)
declare @fromDate1 nvarchar(10)
declare @toDate1 nvarchar(10)
declare @unionKeyword nvarchar(10)
set @unionKeyword=''

DECLARE cursRow CURSOR  FOR 
select tableName,fromDate,toDate from @tableNames
OPEN cursRow
FETCH NEXT FROM cursRow INTO @nameTable,@fromDate1,@toDate1
   


WHILE @@FETCH_STATUS = 0    
BEGIN   

 if(@query&lt;&gt;'')
 begin
set @unionKeyword=' union '
end
 else
 begin
set @unionKeyword=''
end

set  @query =@query + @unionKeyword + N' 

 
(SELECT
	Prc_PCode 
	,prc_date
	,Prc_TakhirLessWork
	,Prc_TajilLessWork
	,isnull(Prc_BeinLessWork,0) as Prc_BeinLessWork 


	,Prc_HourAbsencePure
	,persons.p_name as PersonName,p_family,parts.p_name as PartName
	,P_CustomCode

	FROM ' + @nameTable + '
  join persons on prc_Pcode=p_barcode 
  left outer join parts on persons.P_Parts=parts.P_CustomCode
  WHERE Prc_PCode in (' + @barcode + ') AND  
 SUBSTRING(Prc_Date, 9, 2) &lt;&gt; ''00'' and  prc_date&gt;=''' + @fromDate1 + ''' and prc_date&lt;=''' + @toDate1 + '''
 )'
FETCH NEXT FROM cursRow INTO @nameTable,@fromDate1,@toDate1
END   


CLOSE cursRow   
DEALLOCATE cursRow
set @query='select sum(Prc_TakhirLessWork) as SumTakhirGheirMojaz ,sum(Prc_TajilLessWork) as SumTajilGheirMojaz,
sum(Prc_BeinLessWork) as SumGheibatBeinVaght,sum(Prc_HourAbsencePure) as SumGheibatKhalesSaati
,Prc_PCode,PersonName,p_family,PartName
,P_CustomCode from (' + @query + ') gheibat
group by Prc_PCode,PersonName,p_family,PartName,P_CustomCode '
exec (@query)</SqlCommand>
